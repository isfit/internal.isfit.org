<%= render "transport/nav" %>

<h2>Car <%= @car.id %></h2>
<dl class="dl-horizontal">
  <dt>Navn</dt>
  <dd><%= @car.name %></dd>

  <dt>Beskrivelse</dt>
  <dd><%= @car.description %></dd>

  <dt>Plass</dt>
  <dd><%= @car.space %></dd>

  <dt>Seter</dt>
  <dd><%= @car.seats %></dd>
</dl>

<%= link_to 'Edit', edit_car_path(@car) %> |
<%= link_to 'Back', cars_path %>